<div class="container">
	    <div class="search-box">
	    	<form action="TO-BE-FOUND" method="get" name="search">
	          	<span class="glyphicon glyphicon-search"></span>
	            <input name="search" type="text" value="" />
        	</form>
        	<div class="spacer"></div>
        </div>
        <hr/>
        <div class="news-feed-fake"></div>
        <div class="news-feed">
        {{#each newsfeed-item}}
        	<div class="feed-event">
        		<img class="feed-icon" src="{{imgURL}}"/>
        		<h4>{{title}}</h4>
        		<p class="event-desc">{{description}}</p>
        	</div>
        {{/each}}
        	<div class="feed-event">
        		<div class="feed-img in-progress">
        			<span class="glyphicon glyphicon-gift"></span>
        		</div>
        		<div class="feed-info">
	        		<h4>HCI Work Session</h4>
	        		<p class="event-desc">Yongxing, Jessie and Ben are all in Hog Heaven right now working on their CS 147 assignment!</p>
        		</div>
        		<div class="spacer"></div>
        		<hr/>
        	</div>
        	<div class="feed-event">
        		<div class="feed-img failed">
        			<span class="glyphicon glyphicon-time"></span>
        		</div>
        		<div class="feed-info">
	        		<h4>Lunch Clean</h4>
	        		<p class="event-desc">Ben totally forgot to do his lunch clean.</p>
        		</div>
        		<div class="spacer"></div>
        		<hr/>
        	</div>
        	<div class="feed-event">
        		<div class="feed-img success">
        			<span class="glyphicon glyphicon-time"></span>
        		</div>
        		<div class="feed-info">
	        		<h4>Boba!!!</h4>
	        		<p class="event-desc">We're all going to grab boba in Mountain view this evening!</p>
        		</div>
        		<div class="spacer"></div>
        		<hr/>
        	</div>
        	<div class="feed-event">
        		<div class="feed-img in-progress">
        			<span class="glyphicon glyphicon-gift"></span>
        		</div>
        		<div class="feed-info">
	        		<h4>Coffee Run!</h4>
	        		<p class="event-desc">Harry Simon is borrowing Yongxing's car to go on a coffee run.</p>
        		</div>
        		<div class="spacer"></div>
        		<hr/>
        	</div>
        	<div class="feed-event">
        		<div class="feed-img success">
        			<span class="glyphicon glyphicon-time"></span>
        		</div>
        		<div class="feed-info">
	        		<h4>147 Studio</h4>
	        		<p class="event-desc">Chillin' with Heidi and the crew</p>
        		</div>
        		<div class="spacer"></div>
        		<hr/>
        	</div>
        </div>

	</div>
</div>
